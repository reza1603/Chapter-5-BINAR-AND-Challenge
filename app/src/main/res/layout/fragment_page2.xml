<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Page2Fragment">


    <ImageView
        android:id="@+id/iv_fragment2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_landing_page2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_fragment2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bermain suit melawan komputer."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/iv_fragment2"
        app:layout_constraintStart_toStartOf="@+id/iv_fragment2"
        app:layout_constraintTop_toBottomOf="@+id/iv_fragment2" />
</androidx.constraintlayout.widget.ConstraintLayout>